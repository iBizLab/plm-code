        <association property="ticket" javaType="cn.ibizlab.plm.core.prodmgmt.domain.Ticket" column="owner_id" select="cn.ibizlab.plm.core.prodmgmt.mapper.TicketMapper.selectById" fetchType="lazy"></association>
