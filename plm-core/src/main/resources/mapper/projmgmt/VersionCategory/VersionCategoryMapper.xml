<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper	namespace="cn.ibizlab.plm.core.projmgmt.mapper.VersionCategoryMapper">

        <!--通过mybatis将查询结果注入到entity中,通过配置autoMapping="true"由mybatis自动处理映射关系 -->
    <resultMap id="VersionCategoryResultMap" type="cn.ibizlab.plm.core.projmgmt.domain.VersionCategory" autoMapping="true">
		<id property="id" column="id" /><!--主键字段映射-->
		<result property="createMan" column="create_man" />
		<result property="updateMan" column="update_man" />
		<result property="updateTime" column="update_time" />
		<result property="createTime" column="create_time" />
		<result property="versionSectionId" column="version_section_id" />
        <association property="versionSection" javaType="cn.ibizlab.plm.core.projmgmt.domain.VersionSection" column="version_section_id" select="cn.ibizlab.plm.core.projmgmt.mapper.VersionSectionMapper.selectById" fetchType="lazy"></association>
    </resultMap>

    <select id="selectById"  resultMap="VersionCategoryResultMap">
        <include refid="view" /> 
         where 
         t1.id=#{ id }
    </select>

    <select id="selectEntity" parameterType="cn.ibizlab.plm.core.projmgmt.domain.VersionCategory" resultMap="VersionCategoryResultMap">
        <include refid="view" /> 
         where 
         (t1.id = #{ id })
    </select>

    <select id="selectEntities" parameterType="java.util.List" resultMap="VersionCategoryResultMap">
        <include refid="view" /> 
         where 
         (t1.id)
         in (<foreach collection="list" item="item" index="index" separator=","> (#{ item.id }) </foreach>)
    </select>

    <delete id="deleteEntities" parameterType="java.util.List">
    delete from VERSION_CATEGORY
    where (id)
         in (<foreach collection="list" item="item" index="index" separator=","> (#{ item.id }) </foreach>)
    </delete>

    <sql id="default" databaseId="mysql">
         <![CDATA[ SELECT t1.`CREATE_MAN`, t1.`CREATE_TIME`, t1.`ID`, t1.`NAME`, t1.`UPDATE_MAN`, t1.`UPDATE_TIME`, t1.`VERSION_SECTION_ID` FROM `VERSION_CATEGORY` t1   ]]>  
    </sql>

    <sql id="view" databaseId="mysql">
         <![CDATA[ SELECT t1.`CREATE_MAN`, t1.`CREATE_TIME`, t1.`ID`, t1.`NAME`, t1.`UPDATE_MAN`, t1.`UPDATE_TIME`, t1.`VERSION_SECTION_ID` FROM `VERSION_CATEGORY` t1   ]]>  
    </sql>

    <select id="searchDefault"  parameterType="cn.ibizlab.plm.core.projmgmt.filter.VersionCategorySearchContext"  resultMap="VersionCategoryResultMap">
        select t1.* 
        from (
            <include refid="default"/>
        ) t1 
        <where><if test="ew!=null and ew.sqlSegment!=null and !ew.emptyOfWhere">${ew.sqlSegment}</if></where>
        <if test="ew!=null and ew.sqlSegment!=null and ew.emptyOfWhere">${ew.sqlSegment}</if>
        
    </select>


    <select id="listDefault"  parameterType="cn.ibizlab.plm.core.projmgmt.filter.VersionCategorySearchContext"  resultMap="VersionCategoryResultMap">
        select t1.*
        from (
            <include refid="default"/>
        ) t1
        <where><if test="ew!=null and ew.sqlSegment!=null and !ew.emptyOfWhere">${ew.sqlSegment}</if></where>
        <if test="ew!=null and ew.sqlSegment!=null and ew.emptyOfWhere">${ew.sqlSegment}</if>
        
    </select>

    <select id="findByVersionSectionId" resultMap="VersionCategoryResultMap">
        select t1.* from (
            <include refid="default"/>
        ) t1
        where version_section_id in <foreach item="id" index="index" collection="versionSectionIds" open="(" separator="," close=")">#{id}</foreach> 
    </select>


</mapper>